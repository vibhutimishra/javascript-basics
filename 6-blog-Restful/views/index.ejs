<%- include('header'); -%>

<div class="row">
    <% for (var i=0;i<blogs.length;i++){ %>
        <div class="col-md-3 col-sm-6">
            <div class="thumbnail">
                <img src="<%= blogs[i].image %>"> 
                <div class="caption">
                    <h3><%= blogs[i].title %></h3>
                    <h3><%= blogs[i].body %>.....</h3>
                    <p><%= blogs[i].created %></p>
                    <a href="/blogs/<%= blogs[i]._id %>">Read More!</a>
                </div>   
            </div> 
        </div>
    <%}%>
</div> 

<%- include('footer'); -%> 