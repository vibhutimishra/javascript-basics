<%- include('partials/header'); -%>
<div class="container">
    <div class="jumbotron">
        <h2>Create a new Campground</h2>
    </div>
    <form action="/campgrounds" method="POST">
    <div class="form-group">
        <label for="exampleInputEmail1">Name of the Place:</label>
        <input type="text" class="form-control" name="name aria-describedby="emailHelp" placeholder="Enter name">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1">Image URL</label>
        <input type="text" class="form-control" name="image" placeholder="Image URL">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    </form>
    <button type="button" class="btn btn-success"><a href="/campgrounds">Go back</a></button>
    <%- include('partials/footer'); -%>
</div>
